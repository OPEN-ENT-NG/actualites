{
  "name": "@edifice.io/actualites-tests",
  "version": "1.0.0-develop",
  "description": "Common functions for testing module actualites with K6.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:edificeio/actualites"
  },
  "author": "Edifice",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/edificeio/actualites/issues"
  },
  "homepage": "https://github.com/edificeio/actualites#readme",
  "packageManager": "pnpm@9.7.0",
  "engines": {
    "node": ">=18"
  },
  "engineStrict": true,
  "type": "module",
  "files": [
    "**/*.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:types": "tsc --build",
    "preview": "vite preview",
    "prepare": "husky",
    "clean": "rm -Rf dist",
    "format": "pnpm run format:write && pnpm run format:check",
    "format:check": "npx prettier --check \"src/**/*.ts\"",
    "format:write": "npx prettier --write \"src/**/*.ts\"",
    "pre-commit": "pnpm run format"
  },
  "devDependencies": {
    "@types/k6": "^0.49.3",
    "@types/node": "^20.11.21",
    "edifice-k6-commons": "^2.1.8",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "semantic-release": "^23.0.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^4.3.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,css,md}": "pnpm format"
  }
}
